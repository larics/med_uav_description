<launch>
    
    <!-- Launch UAV -->
    <include file="$(find med_uav_description)/launch/gazebo_bebop.launch">
        <arg name="wind_force" value="0.5"/>
        <arg name="wind_start" default="60.0"/>
        <arg name="wind_duration" default="60.0"/>
        <arg name="wind_direction_x" default="1.0"/>
        <arg name="wind_direction_y" default="1.0"/>
        <arg name="wind_direction_z" default="1.0"/>
    </include>

</launch>
